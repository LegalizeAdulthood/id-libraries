  int max=#maxiter
  int c=0
  int ac=0
  complex z=a
  repeat
    c=c+1
    z=z^@n+@f1(@seed)
  until (c==max)||(|z|>@bailout)
  if c==max
    flag=true
  endif
  ac=c
  if flag==false
    c=0
    z=a1
    repeat
      c=c+1
      z=z^@n+@f1(@seed)
    until (c==max)||(|z|>@bailout)
    if (c>ac)&&(c<max)
      flag2=true
      aa=a1
    endif
    c=0
    z=a2
    if flag2==false
      repeat
        c=c+1
        z=z^@n+@f1(@seed)
      until (c==max)||(|z|>@bailout)
      if (c>ac)&&(c<max)
        flag2=true
        aa=a2
      endif
      c=0
      z=a3
    endif
    if flag2==false
      repeat
        c=c+1
        z=z^@n+@f1(@seed)
      until (c==max)||(|z|>@bailout)
      if (c>ac)&&(c<max)
        flag2=true
        aa=a3
      endif
      c=0
      z=a4
    endif
    if flag2==false
      repeat
        c=c+1
        z=z^@n+@f1(@seed)
      until (c==max)||(|z|>@bailout)
      if (c>ac)&&(c<max)
        flag2=true
        aa=a4
      endif
      c=0
      z=a5
    endif
    if flag2==false
      repeat
        c=c+1
        z=z^@n+@f1(@seed)
      until (c==max)||(|z|>@bailout)
      if (c>ac)&&(c<max)
        flag2=true
        aa=a5
      endif
      c=0
      z=a6
    endif
    if flag2==false
      repeat
        c=c+1
        z=z^@n+@f1(@seed)
      until (c==max)||(|z|>@bailout)
      if (c>ac)&&(c<max)
        flag2=true
        aa=a6
      endif
      c=0
      z=a7
    endif
    if flag2==false
      repeat
        c=c+1
        z=z^@n+@f1(@seed)
      until (c==max)||(|z|>@bailout)
      if (c>ac)&&(c<max)
        flag2=true
        aa=a7
      endif
      c=0

      z=a8
    endif

    if flag2==false
      repeat
        c=c+1
        z=z^@n+@f1(@seed)
      until (c==max)||(|z|>@bailout)

      if (c>ac)&&(c<max)
        flag2=true
        aa=a8

      endif

      c=0
      z=0
    endif
  endif
  if flag==true
    z=#pixel
  else
    z=aa
  endif
endif
loop:

  z = z^@n + @f1(@seed)

bailout:
  |z|<@bailout
  
switch: 
  type="MandelLevels"
  f1=f1
  nothreads=nothreads
  col=col
  n=n
  bailout=bailout
  
default:
  title = "JuliaLevels"
