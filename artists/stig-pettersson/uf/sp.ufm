Quaternion {
init:
  ccolscale=@colscale
  complex z=0
  complex zz=0
  complex zh=0
  complex zzh=0
  complex z1h=0
  bool setzero=false
  float rotx=0
  float roty=0
  float rotz=0
  float x1=0
  float y1=0
  float z1=0
  float rx=0
  float ry=0
  float z1=0
  float zlow=@zvalue-@dist/2
  float zdist=@dist/#magn
  float sinrotx=0
  float cosrotx=0
  float sinroty=0
  float cosroty=0
  float sinrotz=0
  float cosrotz=0
  if @Magnification==0
    zlow=@zvalue-zdist/2
  elseif @Magnification==1
    zlow=@zvalue-zdist

  elseif @Magnification==2
    zlow=@zvalue
  else
    zdist=@dist
  endif
  if @zaxis==0
    int zscreen=#width-1

  elseif @zaxis==1
    int zscreen=#height-1
  else 
    int zscreen=round(#width/@zlength)-1
  endif
  float scale = zdist/zscreen
  int nmax=zscreen+1
  int n=0
  int m=0
  bool nflag=false
  if @fdim>1
    zh=#pixel
  else
    z=#pixel
  endif
  if (@dim2==true)&&(@localdim2==false)
    nmax=1
    zlow=@zvalue
  elseif (@dim2==true)&&(@localdim2==true)
    nmax=1
  endif
if @fdim==3
  while (n<nmax)&&(nflag==false)
    z=scale*n+zlow+@ucoord*1i
    zh=#pixel
    if (@rotx!=0)||(@roty!=0)||(@rotz!=0)
       if @rotx!=0
         if @rotx==90.0
           sinrotx=1.0
           cosrotx=0
         elseif @rotx==-90
           sinrotx=-1.0
           cosrotx=0
         elseif abs(@rotx)==180
           sinrotx=0
           cosrotx=-1
         else
           rotx=#pi*@rotx/180.0
           sinrotx=sin(rotx)
           cosrotx=cos(rotx)
         endif
         y1=imag(zh)
         z1=real(z)
         ry=y1*cosrotx-z1*sinrotx
         rz=y1*sinrotx+z1*cosrotx
         zh=real(zh)+ry*1i
         z=rz+1i*@ucoord
       endif
       if @roty!=0
         if @roty==90.0
           sinroty=1.0
           cosroty=0
         elseif @roty==-90
           sinroty=-1.0
           cosroty=0
         elseif abs(@roty)==180
           sinroty=0
           cosroty=-1
         else
           roty=#pi*@roty/180.0
           sinroty=sin(roty)
           cosroty=cos(roty)
         endif
         z1=real(z)
         x1=real(zh)
         rz=z1*cosroty-x1*sinroty
         rx=z1*sinroty+x1*cosroty
         z=rz+1i*@ucoord
         zh=rx+1i*imag(zh)
       endif
       if @rotz!=0
         if @rotz==90.0
           sinrotz=1.0
           cosrotz=0
         elseif @rotz==-90
           sinrotz=-1.0
           cosrotz=0
         elseif abs(@rotz)==180
           sinrotz=0
           cosrotz=-1
         else
           rotz=#pi*@rotz/180.0
           sinrotz=sin(rotz)
           cosrotz=cos(rotz)

         endif
         x1=real(zh)

         y1=imag(zh)
         rx=x1*cosrotz-y1*sinrotz
         ry=x1*sinrotz+y1*cosrotz
         zh=rx+1i*ry
       endif
     endif 
     zz=z
     zzh=zh
      while (m<#maxiter)&&(|z|+|zh|<@bailout)
      if @hyper==false
        z1h=z*z-real(zh)^2-imag(zh)^2+@c1i
        zh=2*(real(z)*real(zh)+flip(real(z)*imag(zh)))+@cjk
        z=z1h
      else
        z1h=z*z-real(zh)^2-imag(zh)^2-flip(2*real(zh)*imag(zh))+@c1i
        zh=2*(real(z)*real(zh)-imag(z)*imag(zh)+flip(real(z)*imag(zh)+imag(z)*real(zh)))+@cjk
        z=z1h
      endif
      m=m+1
      endwhile
      if (|z|+|zh|)<@bailout
        nflag=true
      else
        nflag=false
      endif    
      m=0
      n=n+1
  endwhile
  z=zz
  zh=zzh
  if @inside==false
    if nflag==true
      z=scale*(n-1)*ccolscale+zlow+@ucoord*1i
      zh=#pixel
    if (@rotx!=0)||(@roty!=0)||(@rotz!=0)
       if @rotx!=0
         y1=imag(zh)
         z1=real(z)
         ry=y1*cosrotx-z1*sinrotx
         rz=y1*sinrotx+z1*cosrotx
         zh=real(zh)+ry*1i
         z=rz+1i*@ucoord 
       endif
       if @roty!=0
         z1=real(z)
         x1=real(zh)
         rz=z1*cosroty-x1*sinroty
         rx=z1*sinroty+x1*cosroty
         z=rz+1i*@ucoord
         zh=rx+1i*imag(zh)
       endif
       if @rotz!=0
         x1=real(zh)
         y1=imag(zh)
         rx=x1*cosrotz-y1*sinrotz
         ry=x1*sinrotz+y1*cosrotz
         zh=rx+1i*ry
       endif
     endif
    endif
  endif
  if (nflag==false)&&(@solid==true)&&(@dim2==false)
    setzero=true
  endif
elseif @fdim==2
  while (n<nmax)&&(nflag==false)
    z=(scale*n+zlow)*1i+@ucoord
    zh=#pixel
    if (@rotx!=0)||(@roty!=0)||(@rotz!=0)
       if @rotx!=0
         if @rotx==90.0
           sinrotx=1.0
           cosrotx=0
         elseif @rotx==-90
           sinrotx=-1.0
           cosrotx=0
         elseif abs(@rotx)==180

           sinrotx=0

           cosrotx=-1
         else

           rotx=#pi*@rotx/180.0
           sinrotx=sin(rotx)

           cosrotx=cos(rotx)
         endif
         y1=imag(zh)
         z1=imag(z)
         ry=y1*cosrotx-z1*sinrotx
         rz=y1*sinrotx+z1*cosrotx
         zh=real(zh)+ry*1i
         z=rz*1i+@ucoord
       endif
       if @roty!=0
         if @roty==90.0
           sinroty=1.0
           cosroty=0
         elseif @roty==-90
           sinroty=-1.0
           cosroty=0
         elseif abs(@roty)==180
           sinroty=0
           cosroty=-1
         else
