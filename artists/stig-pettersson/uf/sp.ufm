  endparam
  
 param InterY
    caption=" u-axis"
    hint="Complement to Iterative Plane.For iter: a-real,b-imag this should be b-real"
    default=0.0
  endparam
  
 param yrot
    caption="Rotates xz-plane."
    hint= "Rotates xyz-plane in degrees around the y-axis."
