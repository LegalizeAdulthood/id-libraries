CubicJulia {
init:
  a=@areal+@aimag*1i
  b=@breal+@bimag*1i
  z=#pixel
loop:
  z=z*z*z-3*a*a*z+b  
bailout:
  |z|<@bailout
default:
  title = "Cubic Julia"
  
  param areal
    caption="a-real"
    hint="Real part of a-parameter"
    default=0.0
  endparam
  
   param aimag
    caption="a-imag"
    hint="Imaginary part of a-parameter"
    default=0.0
  endparam
  
   param breal
    caption="b-real"
    hint="Real part of b-parameter"
    default=0.0
  endparam
  
    
   param bimag
    caption="b-imag"
    hint="Imaginary part of b-parameter"
    default=0.0
  endparam
  
    
   param bailout
    caption="bailout"
    hint="Value of bailout"
    default=100.0  
  endparam
  
}

