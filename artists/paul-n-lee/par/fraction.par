  params=0.28/0.005/3/0.5/0.8/0.6 float=y maxiter=256 inside=0
  decomp=256 periodicity=0 colors=000<61>y99zAAzAAzBB<61>zzzzzz\
  zyz<60>i1ih0hh0hg0g<61>000 cyclerange=0/255
  }

