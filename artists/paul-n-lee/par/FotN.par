frm:FGZ-Julia   { ; (c) Jay Hill, 1998
  ; generalization of formula by Michael G. Wareman
  ; p1 is parameter
  ; p2 is parameter
  ; p3 is focus of Julia set
  z=pixel, c=p3:
  z1=z*z + c;
  z = p1*z1*z1/(z1 + p2) + c;
  |z| <= 16
  }

