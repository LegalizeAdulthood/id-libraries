frm:MandelbrotMix {; Jim Muth
  z=p3, c=pixel,
  a=real(p1), b=imag(p1), d=real(p2),   f=imag(p2):
  z=(a*(z^b))+(d*(z^f))+c,
  |z| <= 100
  } 

