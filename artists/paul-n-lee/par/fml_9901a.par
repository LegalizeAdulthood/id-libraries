                     ;  on P100 1024x768
  reset=1960 type=formula formulafile=frac_ml.frm formulaname=Lesfrm34
  function=cosxx/sin/asin passes=1
  center-mag=0.138443/-0.0508563/0.1011455/1/-44.998 params=5/5/5/5
