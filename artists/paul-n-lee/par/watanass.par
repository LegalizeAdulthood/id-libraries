mongo-12 {  ;SOURCE: mongo.frm
            ;ADD_TO: ORGFORM
  z = pixel, c = (fn1(pixel) * p1):
  x = abs(real(z));
  y = abs(imag(z));
  a = (x <= y) * (fn2(z) + y + c);
  b = (x > y) * (fn2(z) + x + c);
  z = a + b,
  |z| <= p2
}
