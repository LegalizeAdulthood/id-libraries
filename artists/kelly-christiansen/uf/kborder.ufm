kborder7 {
init:
  z = 0    
  p = #pixel/1
  float x = real(p) 
  float y = imag(p) 
  float d = 0
  int iter = 0
loop:
  iter = iter + 1
  d = x
  x = x-.1*sin(sin(y+sin(.7*(y+sin(.7*y)))))
  y = y-.1*cos(cos(d+cos(.7*(d+cos(.7*d)))))
  z = (x^flip(y))

 bailout: 
      |z| <= 100
