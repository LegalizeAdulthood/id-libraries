kborder1 {
init:
  x = real(#pixel) 
  y = imag(#pixel)
  a = real(@a)
  b = imag(@a) 
  d = 0
  int iter = 0
loop:
  iter = iter + 1
  d = x
  x = x - a * sin(y)
  y = y - b * sin(d)
  z = x + flip(y)
bailout:
  |x| < @b
default:
  center=(23.3928882094294254,-2.34344328983381394)
  maxiter = 20 
  magn = 0.0603068
  angle=90
  param a
    caption = "Value a"
    default = (1,1)
  endparam 
  param b
  caption ="Value b"
  default = 20
  endparam 
}

