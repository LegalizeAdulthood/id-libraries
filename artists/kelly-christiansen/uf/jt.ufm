JT8 {
init:
  z = 0
loop:
 z = z * (0.3i)^z + z^2 + #pixel
bailout:
  |z| < 2  
default:
  title = "Big Spiral"
param bailout
    caption = "Bailout value"
    default = 5.0
    min = 1.0
endparam
}


JT9 {
init:
  z=0
  n=#pixel
loop:
  z  = z*z + (z+n)*z*z - sin(z) + n
bailout:
  |z|<24
default:
  title = "Dragon"
param bailout
    caption = "Bailout value"
    default = 5.0
    min = 1.0
endparam
}


JT20 {
z = #pixel 
loop:
  z = z^5 + tanh(z)
bailout:
  |z| < 4
default:
  title = "Quantum"
  center = (0.0, 0.0)
  magn = 1.5
  maxiter = 500
param bailout
    caption = "Bailout value"
    default = 5.0
    min = 1.0
endparam
}

JT20b {
z = #pixel 
loop:
  z = z^5*@fn1(z) + tanh(z)
bailout:
  |z| < 4
default:
  title = "Quantum2"
  center = (0.0, 0.0)
  magn = 1.5
  maxiter = 500
func fn1
   caption = "Function 1"
   default = log()
endfunc
param bailout
    caption = "Bailout value"
    default = 5.0
    min = 1.0
endparam
}


