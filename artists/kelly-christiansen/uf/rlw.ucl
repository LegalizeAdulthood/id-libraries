suspense {;2002 rlw feb 24
init:
float c = 0.01
float b = 0.01
float d = 0.0
loop:
      c=c/b
      b= b+0.01
      d=d+real(@f1(#z*c) / b)
final:
      #index = (d%@m)/@m
default:
  title = "suspense"
 
  func f1
    caption="zc"
    default=sin()
    endfunc
param m
caption="colors"
default=400
endparam
