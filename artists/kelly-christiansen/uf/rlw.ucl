blame {;2002 rlw
init:
float c = 24.0/31.0
float d = 0.0
int i=1
loop:
      c=c^2-1
      if i<=@si
      d=0
      else
      d=(real(fn1(#z))-imag(fn1(#z)))*c
      endif
      i=i+1
final:
      #index=(d%@m)/@m
default:
  title = "blame"
 param m
 caption="colors"
 default=400
 endparam
  param si
  caption="skip iters"
  default=0
  endparam
}

