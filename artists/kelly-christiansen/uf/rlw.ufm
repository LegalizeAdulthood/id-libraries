shelbie{;russalynn mar 11,  2002
init:
  z = c = pixel
loop:
if @b==0
  z = real(c)^2+imag(c)
  elseif @b==1
  z =  real(c)^2-imag(c)
  elseif@b==2
  z =  real(c)^2*imag(c)
  elseif@b==3
  z = real(c)^2/imag(c)
  elseif@b==4
  z =  real(c)^2^imag(c)
  endif
  if@a==0
  c=c+(sin(fn1(z))/cos(c))
  elseif@a==1
  c=c-(sin(fn1(z))/cos(c))
  elseif@a==2
  c=c*(sin(fn1(z))/cos(c))
  elseif@a==3
  c=c/(sin(fn1(z))/cos(c))
  elseif@a==4
  c=c^(sin(fn1(z))/cos(c))
  endif
bailout:
  |z| <= @bailout
default:
  title = "shelbie"
  magn=0.5
  periodicity=0
  param bailout
 caption = "bailout condition"
 default = 10.0 
 min = 1E-10 
endparam
param a
caption="you decide"
enum="+""-""*""/""^"
endparam
param b
caption="decisions, decisions"
enum="+""-""*""/""^"
default=0
endparam
}

