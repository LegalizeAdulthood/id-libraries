evwillar-01{
;Use param settings
;to get interesting variations
init:
  z=#pixel
  a=@ma
  b=@mb
  c=#pixel
loop:
  z=a+b+z^3-b*z+c
  
bailout:
  |z|<@bailout
default:
  title = "willar-01"
PARAM MA
  default=(1.0,0.0)  
  caption="coeff A" 
  Hint="A in a+b+z^3-bz+c" 
  endparam
param MB
  default=(1.0,0.0)
  caption="coeff B"
  hint="b in a+b+z^3  -bz+c"
  endparam
param bailout
 caption="bailout"
 default=4.0
 min=1e-20
endparam
}

