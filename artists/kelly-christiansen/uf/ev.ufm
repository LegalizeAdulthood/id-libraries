kate-04{
;this really belongs to Rui
;I used some of his
;tutorial, etc settings
init: 
  a=1 
  b=2 
  c = #pixel  
  if @ca == 0 
  z=#pixel 
  elseif @ca == 1 
  z = -2/(3*b) 
  elseif @ca == 2 
  z= @cut 
  endif 
loop:
  z=z*z*z+a+b^2+c*(z+2)+#pixel
  
bailout:
  |z|<@bailout
default:
  title = "kate-04"
  center=(-1.345,-0.01)
  magn=3.44828
  
param CA 
 enum="pixel" "critical" "user" 
 default=0 
 endparam 
 param cut
 default = (0,0) 
 caption = "Cut plane"
endparam

param bailout
 caption="bailout"
 default=4.0
 min=1e-20
endparam
}

