}

Mandy {
init:
  z=1/((2*pixel-fn1(5/pixel))+fn2(pixel-fn1(2.5/pixel)))
  c=1/pixel+fn1(flip(tan(z*z)))
loop:
  z=z*z+c
bailout:
  |real(z)| <= 4
default:
  title = "Mandy"
  periodicity = 0
  maxiter = 500
  magn = .7
  center = (1.38,0)

func fn1
  caption = "Here"
  default = zero ()
endfunc

func fn2
  caption = "There"
  default = ident ()
endfunc
}

Oludeniz {
init:
  z = pixel + 1
loop:
  z = fn1(z * z) * ( 1 / z ^ z ) + pixel
bailout:
  |z| <= 4
default:
  title = "Oludeniz"
  periodicity = 0
  maxiter = 500
}

Raven {
init:
  z = pixel + 1
loop:
  z=(z/imag(z)-z/real(z))*p1*fn1(z)+p2
bailout:
  |z| <= @bailout
default:
  title = "Raven"
  method = multipass
  maxiter = 100
  periodicity = 0
  center = (-1, 0)
  
param bailout
  caption = "Bailout"
  default = 10
endparam

param p1
  caption = "1st Multiplier"
  default = (-1.0, 0)
endparam

param p2
  caption = "2nd Multiplier"
  default = (-0.5, -0.4)
endparam

func fn1
  caption = "First Raven"
  default = ident ()
endfunc
}
