Follies1 {
init:
  z = Pixel
loop:
  z = fn1(z * z + 1) + P1
bailout:
  |z| <= 4
default:
  title = "Follies1"
  method = multipass
  
func fn1
  caption = "Follies"
  default = asin()
endfunc
}

Follies2 {
init:
  z = Pixel+1
loop:
  z = fn1(z + z * z + 1) + P1
bailout:
  |z| <= 4
default:
  title = "Follies2"
  method = multipass
func fn1
  caption = "Follies"
  default = asin()
