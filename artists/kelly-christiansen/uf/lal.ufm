Follies5 {
init:
  z = c = pixel
loop:
  z = fn1( z * z ) / (( z * z + z ) + fn2(( c + z) * z )* fn3( z )) + pixel
bailout:
  |z| <= @bailout
default:
  title = "Follies5"
  method = multipass
  
param bailout 
 caption = "bailout" 
 default = 4.0 
 min = 1E-10  
endparam

func fn1 
  caption = "Follies"
  default = log()
endfunc
func fn2
  caption = "More Follies"
  default = ident()
endfunc
func fn3
  caption = "And More Follies"
  default = ident()
endfunc
}

