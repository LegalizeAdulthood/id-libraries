 elseif @type1 == "^"
   j=(fn3(@p3+z)^fn4(@p4+z))^(z+c)
 endif
endif
 
if @op3 == "+"   
 if @type2 == "+"
  z=@fn5(z*@p5)+(w*j)+x
 elseif @type2 == "-"
