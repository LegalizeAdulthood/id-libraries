    cz2=real(@p2)*p4i 
    cw1=flip(imag(@p2))*p5r 
    cw2=imag(@p2)*p5i 
  endif 
  endif 


Loop:

  ;Define the sets Normal and Cubic of a Triternion.
  if (@selmult==0)                                                                                 ;3D-Mandelbrot
   a1=z1^2+z2^2+2*w1*w2+2*v1*v2 
   a2=2*z1*z2+2*w1*v2+2*w2*v1 
   b1=w2^2+v2^2+2*z1*w1+2*z2*v1 
   b2=w1^2+v1^2+2*z1*w2+2*z2*v2 
   c1=2*z1*v1+2*z2*w1+2*w2*v2 
   c2=2*z1*v2+2*z2*w2+2*w1*v1
  elseif (@selmult==1)                                                                             ;3D-Cubic Mandelbrot
   a1=z1^3+w1^3+w2^3+3*(z1*z2^2+w1*v1^2+w2*v2^2)+6*(z1*(w1*w2+v1*v2)+z2*(w1*v2+w2*v1))
   a2=z2^3+v2^3+v1^3+3*(z2*z1^2+v1*w1^2+v2*w2^2)+6*(z1*(w1*v2+w2*v1)+z2*(w1*w2+v1*v2))
   b1=3*(w2*(w1^2+v1^2)+z1*(w2^2+v2^2)+w1*(z1^2+z2^2))+6*(z2*w2*v2+z1*z2*v1+w1*v1*v2)   
   b2=3*(w1*(w2^2+v2^2)+z1*(w1^2+v1^2)+w2*(z1^2+z2^2))+6*(z2*w1*v1+v1*w2*v2+z1*z2*v2) 
   c1=3*(v1*(z1^2+z2^2)+z2*(w2^2+v2^2)+v2*(w1^2+v1^2))+6*(z1*w1*z2+z1*w2*v2+w1*v1*w2)
   c2=3*(v2*(z1^2+z2^2)+z2*(w1^2+v1^2)+v1*(w2^2+v2^2))+6*(z1*z2*w2+z1*w1*v1+w1*w2*v2)
  endif 

   a1=fn1(a1)  
   a2=fn2(a2) 
   b1=fn3(b1) 
   b2=fn4(b2) 

   z1=(a1+cz1) 
   z2=(a2-cz2) 
   w1=(b1+cw1) 
   w2=(b2-cw2) 
   v1=(c1+cv1) 
   v2=(c2-cv2) 

   z1=@fn5(z1) 
   z2=@fn6(z2) 
   w1=@fn7(w1) 
   w2=@fn8(w2) 

  if (@flavor == 0) 
   z=z1-z2+flip(w1-w2) 
  elseif (@flavor == 1) 
   z=z1-z2-(w1+w2) 
  endif 

