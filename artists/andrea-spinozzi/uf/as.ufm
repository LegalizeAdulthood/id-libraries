   z=#pixel 
   float cr=real(@seed*p1r) 
   float ci=imag(@seed*p1i) 
   float tx=0.0 
   float ty=0.0 
  endif 

loop: 

 if (@invert==true) 
  z=recip(z) 
 endif 

 float x=real(fn1(z)*p3) 
 float y=imag(fn2(z)*p4) 

if (@op == 0) 
if (@op1 == 0) 
 tx=cr - imag(fn3(y)^p2i) + y 
elseif (@op1 == 1) 
 tx=cr + imag(fn3(y)^p2i) + y 
elseif (@op1 == 2) 
 tx=cr / imag(fn3(y)^p2i) + y 
elseif (@op1 == 3) 
