        af1 = (5*@a+@b)*@afn1(theta) - @b*@afn1((5*@a+@b)*theta/@b)
        af2 = (5*@a+@b)*@afn2(theta) - @b*@afn2((5*@a+@b)*theta/@b)
      polar = 0.5*(af1 + flip(af2))
      d1 = abs(cabs(z1)-cabs(polar)) ; get current distances
