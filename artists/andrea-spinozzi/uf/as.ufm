   tw=z^p3i-b
  elseif (@op==27)
   tz=b^p3r-a
   tw=z^p3i*b
  elseif (@op==28)
   tz=b^p3r*b
   tw=z^p3i+a
  elseif (@op==29)
   tz=b^p3r*b
   tw=z^p3i-a
  endif
  
    tz=tz*ca
