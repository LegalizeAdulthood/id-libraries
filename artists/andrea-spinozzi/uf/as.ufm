As-QuatMan {
;QuatMan_yz
;4-dimensional M-Set
;Original formula in the orgform collection , _q.frm.
;Andrea Spinozzi(May2004)

Init:

  z=real(p1) + flip(real(pixel))
  cz=real(p1) + flip(real(pixel))
  cw=imag(pixel) + flip(imag(p1))
  w=imag(pixel) + flip(imag(p1))
  qz=sqr(z)
  qw=sqr(w)
  p2r=real(p2)
  p2i=imag(p2)
  
 if (@mode == "Mandelbrot" ) 
  z=real(p1) + flip(real(pixel))
  cz=real(p1) + flip(real(pixel))
  cw=imag(pixel) + flip(imag(p1))
  w=imag(pixel) + flip(imag(p1))
  qz=sqr(z)
  qw=sqr(w)
  p2r=real(p2)
  p2i=imag(p2)
 elseif (@mode == "Julia")
  if (@varJulia == 0)
    z=real(p1) + flip(real(pixel))
    cz=real(p1) + flip(real(pixel))
    cw=imag(@seed) + flip(imag(p1))
    w=imag(pixel) + flip(imag(p1))
    qz=sqr(z)
    qw=sqr(w) 
    p2r=real(p2)
    p2i=imag(p2)
  elseif (@varJulia == 1)
    z=real(p1) + flip(real(pixel))
    cz=real(p1) + flip(real(pixel))
    cw=imag(pixel) + flip(imag(p1))
    w=imag(@seed) + flip(imag(p1))
    qz=sqr(z)
    qw=sqr(w)
    p2r=real(p2)
    p2i=imag(p2)
  elseif (@varJulia == 2)
    z=real(p1) + flip(real(pixel))
    cz=real(p1) + flip(real(@seed))
    cw=imag(@seed) + flip(imag(p1))
    w=imag(pixel) + flip(imag(p1))
    qz=sqr(z)
    qw=sqr(w)
    p2r=real(p2)
    p2i=imag(p2)
  elseif (@varJulia == 3)
    z=real(p1) + flip(real(pixel))
    cz=real(p1) + flip(real(@seed))
    cw=imag(pixel) + flip(imag(p1))
    w=imag(@seed) + flip(imag(p1))
    qz=sqr(z)
    qw=sqr(w)
    p2r=real(p2)
    p2i=imag(p2)
  elseif (@varJulia == 4)
    z=real(p1) + flip(real(@seed))
    cz=real(p1) + flip(real(pixel))
    cw=imag(@seed) + flip(imag(p1))
    w=imag(pixel) + flip(imag(p1))
    qz=sqr(z)
    qw=sqr(w)
    p2r=real(p2)
    p2i=imag(p2)
  elseif (@varJulia == 5)
    z=real(p1) + flip(real(pixel))
    cz=real(p1) + (real(@seed))
    cw=imag(pixel) + flip(imag(p1))
    w=imag(@seed) + flip(imag(p1))
    qz=sqr(z)
    qw=sqr(w) 
    p2r=real(p2)
    p2i=imag(p2)
  elseif (@varJulia == 6)
    z=real(p1) + flip(real(pixel))
    cz=real(p1) + (real(@seed))
    cw=imag(@seed) + flip(imag(p1))
    w=imag(pixel) + flip(imag(p1))
    qz=sqr(z)
    qw=sqr(w) 
    p2r=real(p2)
    p2i=imag(p2)
  endif 
 endif
 

Loop:

  if (@mode1 == 0)
   tz=fn1(qz)-fn2(qw)+fn3(cz)
   tw=2*z*w+cw
   z=fn4(tz)
   w=@fn5(tw)
