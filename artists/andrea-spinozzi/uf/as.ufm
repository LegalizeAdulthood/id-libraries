   c=@startSeed
   if @scale != 1.0
    z=z/@scale
   endif
   if @center != (0,0)
    z=z + @center
   endif
  endif

Loop:

 if (@invert == true)
 z=1/z
 endif 

 j=real(atan(z))
 w=imag(atan(z))
 d=fn1(c)
 f=recip(z)
 g=fn2(z)
 h=fn3(c)

if @op=="1"
 z=(((d*g)*cotan(@p)*z)+(h*c))*atan(p1)
elseif @op=="2"
 z=(((d*g)*cotan(p2)*z)+((f*h)*sqr(z))*c)*atan(p3)
elseif @op=="3"
 z=(((d*g)*cotan(p4)*z)*((f*h)/sqr(z))*c)*atan(p5)
elseif @op=="4"
 z=(((d*g)*cotan(@p8)*z)+(h+sqr(c)))*atan(@p9)
elseif @op=="5"
