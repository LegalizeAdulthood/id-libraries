    z6=(z5+z0)-z5*zri
   elseif (@op1==1)
    z6=(z5+z0+z1)-z5*zri
   elseif (@op1==2)
    z6=(z5+z0+z1+z2)-z5*zri
   elseif (@op1==3)
    z6=(z5+z0+z1+z2+z3)-z5*zri
   elseif (@op1==4)
    z6=(z5+z0+z1+z2+z3+z4)-z5*zri
   endif
   elseif (@select==4)
   if (@op1==0)
    z6=(z5+z0)*z5-zri
   elseif (@op1==1)
    z6=(z5+z0+z1)*z5-zri
   elseif (@op1==2)
