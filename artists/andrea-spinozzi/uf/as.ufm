   elseif (@op1==4)
    b=p2-a*b+p1
   elseif (@op1==5)
    b=p2*a+b+p1
   elseif (@op1==6)
    b=p2*a-b+p1
   endif
   
   if (@flavor==0)
    z=fn3(a)+fn4(b)
   elseif (@flavor==1)
    z=fn3(a)-fn4(b)
   endif
   
