Loop:
   
   a1=fn1(z1-w1*(0,1))
   a2=fn1(z2-w2*(0,1))
   a3=fn1(z3-w3*(0,1))
   b1=fn1(z1+w1*(0,1))
   b2=fn1(z2+w2*(0,1))
   b3=fn1(z3+w3*(0,1))
                
    ca1=(a1+real(p6))^p3
    ca2=(a2+real(p6))^p3
    ca3=(a3+real(p6))^p3
