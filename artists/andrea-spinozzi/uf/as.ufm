      endif
    elseif (@zmode == 27)                  ; smallest Tractrix
      theta = atan(imag(#z)/real(#z))
        af1 = @a*(log(tan(theta/2))-@afn1(theta))
        af2 = @a*@afn2(theta)
      polar = 0.25*(af1 + flip(af2))
      d1 = abs(cabs(z1)-cabs(polar)) ; get current distances
      d2 = abs(cabs(z2)-cabs(polar))
