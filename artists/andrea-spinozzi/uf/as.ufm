As-Popcorn {
;Andrea Spinozzi(July2004)


Init: 

  complex z=(0.0,0.0)
  complex c=(0.0,0.0)
  complex x=(0.0,0.0)
  complex y=(0.0,0.0)
  complex a=(0.0,0.0)
  complex b=(0.0,0.0)
  complex a1=(0.0,0.0)
  complex b1=(0.0,0.0)
 
 if (@mode=="Mandelbrot")
  z=(0.0,0.0)
  c=pixel
  x=real(pixel)
  y=imag(pixel)
 elseif (@mode=="Julia")
  z=pixel
  c=@seed
 if (@variant==0)
  x=real(pixel)
  y=imag(pixel)
 elseif (@variant==1)
  x=real(pixel)*@p5
  y=imag(pixel)*@p5
 endif
 endif

loop: 

 if (@op1==0)
  a=real(p1*fn1(y+fn2(p2*y)))
  b=imag(p1*fn3(x+fn4(p2*x)))
  a1=real(p1*fn3(x+fn4(p2*x)))
  b1=imag(p1*fn1(y+fn2(p2*y)))
 elseif (@op1==1)
  a=-real(p1*fn1(y-fn2(p2*y)))
  b=-imag(p1*fn3(x-fn4(p2*x)))
  a1=-real(p1*fn3(x-fn4(p2*x)))
  b1=-imag(p1*fn1(y-fn2(p2*y)))
 endif

 if (@op==0)
  x=(x-a-b)-c*(a*b)
  y=(y-a1-b1)-c*(a1*b1)
 elseif (@op==1)
  x=(x-a-b)*c-(a*b)
  y=(y-a1-b1)-c*(a1*b1)
 elseif (@op==2)
  x=(x-a-b)-c*(a*b)
  y=(y-a1-b1)*c-(a1*b1)
 elseif (@op==3)
  x=(x-a-b)*c-(a*b)
  y=(y-a1-b1)*c-(a1*b1)
 elseif (@op==4)
  x=(x-a-b)+c*(a-b)
  y=(y-a1-b1)+c*(a1-b1)
 elseif (@op==5)
  x=(x-a-b)+c*(a-b)
  y=(y-a1-b1)+c*(a1*b1)
 elseif (@op==6)
  x=(x-a-b)+c*(a*b)
  y=(y-a1-b1)+c*(a1-b1)
 elseif (@op==7)
  x=(x-a-b)-c*(a-b)
  y=(y-a1-b1)-c*(a1-b1)
 elseif (@op==8)
  x=(x-a-b)+c*(a-b)
  y=(y-a1-b1)-c*(a1-b1)
 elseif (@op==9)
  x=(x-a+b)+c*(a+b)
  y=(y-a1+b1)+c*(a1+b1)
 endif
 
  x=@fn5(x)^p3
  y=@fn6(y)^p4
 
  if (@flavor==0)
   z=@fn7(x)+y
  elseif (@flavor==1)
   z=@fn7(x)-flip(y)
  endif
 
bailout: 

 |x|+|y|<=@bailout 

