   float cr=0.0 
   float ci=0.0 
   float tx=0.0 
   float ty=0.0 
   p1r=real(p1) 
   p1i=imag(p1) 
   p2r=real(p2) 
   p2i=imag(p2) 


  if (@mode == "Mandelbrot") 
   z=#pixel 
   float cr=real(z*p1r) 
   float ci=imag(z*p1i) 
   float tx=0.0 
   float ty=0.0 
