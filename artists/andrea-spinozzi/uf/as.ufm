 elseif @select==5
  z=(z+c)+(x+y)
 elseif @select==6
  z=(z+c)-(x+y)
 elseif @select==7
  z=(z+c)*(x+y)
 elseif @select==8
  z=(z-c)+(x+y)
 elseif @select==9
