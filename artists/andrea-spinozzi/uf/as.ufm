Potter {
;Andrea Spinozzi(Oct2003)

Init:

 z=@startseed
 c=#pixel
 x=#pixel
 y=#pixel
 bool k=true
 stop=0

if @mode=="Mandelbrot"
 z=@StartSeed
 c=#pixel
 endif
if @mode=="Julia"
 z=#pixel
 c=@StartSeed
 if @scale != 1.0
    z=z/@scale
 endif
 if @center != (0,0)
    z=z+@center
 endif
endif

Loop:


if @op=="1"
 x=(fn1(c^p1^1.5/p2)*fn2(z/p1/1.5^p2))*|z|
 y=(fn3(c^p2*p1*1.5)*cos(z/p1/1.5^p2))*|z|
elseif @op=="2"
 x=(fn1(c^p1^1.5/p2)+fn2(z/p1/1.5^p2))*|z|
 y=(fn3(c^p2*p1*1.5)+cos(z/p1/1.5^p2))*|z|
elseif @op=="3"
 x=(fn1(c^p1^1.5/p2)-fn2(z/p1/1.5^p2))*|z|
 y=(fn3(c^p2*p1*1.5)-cos(z/p1/1.5^p2))*|z|
endif
;----------------------------------
if @test==0
 k=|z|<|@bailout| 
elseif @test==1
