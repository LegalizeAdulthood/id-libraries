      ; and not all functions apply to floats
      if (@xfer == 1); log
        e1 = log(e1)
        e2 = log(e2)
        e3 = log(e3)
      elseif (@xfer == 2); sqrt
        e1 = sqrt(e1)
        e2 = sqrt(e2)
        e3 = sqrt(e3)
      elseif (@xfer == 3); cuberoot
